<%- include('user-header'); %>
<%- include('user-sidemenu'); %>
<!-- Content
	================================================== -->
	<div class="dashboard-content">
        <div class="row">
            <div class="col-md-12">
                <br>
                Property Entry : 90%
                <div id="progress-bar" style="width:90%;"></div>
                 
            </div>
        </div>
		<div class="row">
			<div class="col-lg-12">

				<div id="add-listing">

					<!-- Section -->
					<div class="add-listing-section margin-top-30">

						<!-- Headline -->
						<div class="add-listing-headline">
							<h3><i class="im im-icon-Blueprint"></i>Additional Information (optional)</h3>
						</div>

						<!-- Row -->
						<div class="row with-forms">

							<!-- Phone -->
							<div class="col-md-6">
								<h5>Do you Have Deed Sale Certificate? <span>*</span></h5>
								<select name="certificate">
									<option>Select Certificate</option>
									<option>Agra</option>
									<option>Delhi</option>
									<option>Kanpur</option>
									<option>Lucknow</option>
								</select>
								<input type="hidden" name="property_id" value="<%=obj.property_id%>">
							</div>
							<!-- Website -->
							<div class="col-md-6">
								<h5>Have you Paid Property Tax? <span>*</span></h5>
								<select name="certificate">
									<option>Select Property Tax</option>
									<option>Agra</option>
									<option>Delhi</option>
									<option>Kanpur</option>
									<option>Lucknow</option>
								</select>
							</div>

							

						</div>
						<!-- Row / End -->
						
						<!-- Row -->
						<div class="row with-forms">

							<!-- Email Address -->
							<div class="col-md-6">
								<h5>Do you Have Occupancy Certificate? <span>*</span></h5>
								<select name="bhk">
									<option>Select Certificate</option>
									<option>1 BHK</option>
									<option>2 BHK</option>
									<option>3 BHK</option>
									<option>4 BHK</option>
								</select>
							</div>
							<button type="submit" class="button preview pull-right">Submit <i class="fa fa-arrow-circle-right"></i></button>

						</div>
						<!-- Row / End -->
						
						
						
					</div>
					<!-- Section / End -->


					

				</div>
			</div>
<%- include('user-footer'); %>