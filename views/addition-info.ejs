<%- include('user-header'); %>
<%- include('user-sidemenu'); %>
<!-- Content
	================================================== -->
	<div class="dashboard-content">
        <div class="row">
            <div class="col-md-12">
                <br>
                Property Entry : 90%
                <div id="progress-bar" style="width:90%;"></div>
                 
            </div>
        </div>
		<div class="row">
			<div class="col-lg-12">

				<div id="add-listing">

					<!-- Section -->
					<div class="add-listing-section margin-top-30">

						<!-- Headline -->
						<div class="add-listing-headline">
							<h3><i class="im im-icon-Blueprint"></i>Additional Information (optional)</h3>
						</div>
						<form action="/property/add_additional_info" method="POST" id="add_info">
						<!-- Row -->
						<div class="row with-forms">
							<% for(var a=0;a<obj.certification_type.length;a++){ %>
							<!-- Phone -->
							<div class="col-md-6">
								<h5><%=obj.certification_type[a].certification_name%></h5>
								<select name="certificate">
									<option value="">Select Certificate</option>
									<option value="yes" <%if(obj.all_cerfit.length>0){if(obj.all_cerfit[a].certification_type_value=='yes'){%> selected<% }} %>>Yes</option>
									<option value="donot know" <%if(obj.all_cerfit.length>0){if(obj.all_cerfit[a].certification_type_value=='donot know'){%> selected<% }} %>>Don't know</option>
								</select>
							</div>
							<% } %>
							<input type="hidden" name="property_id" value="<%=obj.property_id%>">
						</div>
						<div class="row with-forms">

							<button type="submit" id="sbt_btn" class="button preview pull-right">Submit <i class="fa fa-arrow-circle-right"></i></button>
								
							</div>
						<!-- Row / End -->
					</div>
				</form>
					<!-- Section / End -->
				</div>
			</div>
			<script>
				$('document').ready(function(){
				$('#sbt_btn').on('click',function(e){
				e.preventDefault();
				$('#add_info').submit();
				});
				});
			</script> 
<%- include('user-footer'); %>