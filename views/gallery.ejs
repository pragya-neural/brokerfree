<%- include('user-header'); %>
<%- include('user-sidemenu'); %>
<!-- Content
	================================================== -->
	<div class="dashboard-content">
        <div class="row">
            <div class="col-md-12">
                <br>
                Property Entry : 45%
                <div id="progress-bar" style="width:45%;"></div>
                 
            </div>
        </div>
		<div class="row">
			<div class="col-lg-12">

				<div id="add-listing">

				<!-- Section -->
					<div class="add-listing-section margin-top-30">

						<!-- Headline -->
						<div class="add-listing-headline">
							<h3><i class="sl sl-icon-picture"></i> Get 100% More Response By adding Photos</h3>
						</div>

						<!-- Dropzone -->
						<div class="submit-section">
							<form action="/property/update-property-images/<%=obj.property_id%>" enctype="multipart/form-data" id="update_property_image" method="post">
							<input type="file"  name="update_property_image" multiple accept=".jpeg,.png,.jpg,.pdf">
							
						</div>

					</div>
					<!-- Section / End -->

						
						<!-- Row -->
						<div class="row with-forms">

							<button type="submit" id="sbt_btn" class="button preview pull-right">Submit <i class="fa fa-arrow-circle-right"></i></button>
						
						</div>
						<!-- Row / End -->

						</form>

					</div>
					<!-- Section / End -->


					

				</div>
			</div>
			<script>
				$('document').ready(function(){
				$('#sbt_btn').on('click',function(e){
				e.preventDefault();
				$('#update_property_image').submit();
				});
				});
			</script> 
            <%- include('user-footer'); %>